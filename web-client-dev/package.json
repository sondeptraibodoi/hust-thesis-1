    "react-icons": "^4.10.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-pdf": "9.1.1",
    "react-redux": "^8.1.2",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.14.2",
    "react-sortablejs": "^6.1.4",
    "react-zoom-pan-pinch": "^3.6.1",
    "recharts": "^2.12.7",
    "sass": "^1.64.2",
    "sortablejs": "^1.15.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
