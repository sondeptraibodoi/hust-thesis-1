    "chart.js": "^4.5.0",
    "commitizen": "^4.3.0",
    "core-js": "^3.37.1",
    "date-fns": "^2.30.0",
    "dayjs": "1.11.9",
    "i18next": "^23.4.4",
    "i18next-chained-backend": "^4.4.0",
    "i18next-http-backend": "^2.2.2",
    "i18next-localstorage-backend": "^4.1.1",
    "i18next-resources-to-backend": "^1.1.4",
    "katex": "^0.16.10",
    "laravel-echo": "^1.16.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pdfjs-dist": "4.4.168",
